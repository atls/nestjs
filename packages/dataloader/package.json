    "postpack": "rm -rf dist"
  },
  "dependencies": {
    "dataloader": "2.2.2"
  },
  "devDependencies": {
    "@nestjs/common": "10.4.1",
    "@nestjs/core": "10.4.1",
    "@nestjs/graphql": "12.2.0",
    "@nestjs/microservices": "10.4.1",
    "@types/lodash": "4.17.7",
    "@types/node": "22.5.5",
    "graphql": "16.9.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "ts-morph": "19.0.0",
    "ws": "8.14.1"
  },
  "peerDependencies": {
    "@nestjs/common": "10",
    "@nestjs/core": "10",
    "@nestjs/graphql": "12",
    "graphql": "16",
    "reflect-metadata": "0.2",
    "rxjs": "7"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "main": "dist/index.js",
    "typings": "dist/index.d.ts"
  }
}
