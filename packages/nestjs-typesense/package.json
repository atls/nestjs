  "devDependencies": {
    "@jest/globals": "29.7.0",
    "@nestjs/common": "10.4.1",
    "@nestjs/core": "10.4.1",
    "@nestjs/testing": "10.4.1",
    "@types/node": "22.5.5",
    "get-port": "7.1.0",
    "reflect-metadata": "0.2.2",
    "rxjs": "7.8.1",
    "typesense": "1.8.2"
  },
  "peerDependencies": {
    "@nestjs/common": "10",
    "@nestjs/core": "10",
    "reflect-metadata": "0.2",
    "rxjs": "7",
    "typesense": "^0.15"
  },
  "publishConfig": {
    "exports": {
      "./package.json": "./package.json",
      ".": {
